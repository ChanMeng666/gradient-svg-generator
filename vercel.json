{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/svg",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "image/svg+xml"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}